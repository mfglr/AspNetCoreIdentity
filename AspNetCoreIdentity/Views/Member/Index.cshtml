@using AspNetCoreIdentity.ViewModels;

@{
    ViewData["Title"] = "Index";
}
@model UserViewModel

<div class="row">
    <div class="col-md-6 offset-md-3">
        <h1>Welcome @User.Identity!.Name</h1>

        <table class="table">
            <thead>
                <tr>
                    <th>UserName</th>
                    <th>Email</th>
                    <th>PhoneNumber</th>
                </tr>
            </thead>

            <tbody>

                <tr>
                    <td>@Model.UserName</td>
                    <td>@Model.Email</td>
                    <td>@Model.Phone</td>
                </tr>

            </tbody>


        </table>


    </div>
</div>




